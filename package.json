{
  "private": true,
  "scripts": {
    "build": "next build",
    "check-all": "yarn lint && yarn build && yarn format:check && yarn typecheck && yarn commitlint && yarn stylelint",
    "commitlint": "commitlint --from=HEAD~1",
    "dev": "next dev",
    "format": "yarn prettier --write",
    "format:check": "yarn prettier --check",
    "gcb": "bash ./scripts/shell/git-create-branch.sh",
    "gcmwp": "bash ./scripts/shell/git-checkout-main-with-pull.sh",
    "gpc": "bash ./scripts/shell/git-push-changes.sh",
    "grlc": "bash ./scripts/shell/git-reset-local-changes.sh",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "reinstall-node-modules": "bash ./scripts/shell/reinstall-node-modules.sh",
    "start": "next start",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx)\"",
    "typecheck": "tsc --noEmit -p . --pretty"
  },
  "dependencies": {
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "autoprefixer": "^10.4.0",
    "eslint": "8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "12.0.8",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "inquirer-directory": "^2.2.0",
    "lint-staged": "^12.3.1",
    "plop": "^3.0.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-package-json": "^2.6.0",
    "tailwindcss": "^3.0.7",
    "typescript": "4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.17"
  }
}
